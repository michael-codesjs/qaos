{
  "name": "@qaos/be",
  "version": "1.0.0",
  "description": "Backend service for qaos",
  "main": "dist/src/index.js",
  "license": "MIT",
  "scripts": {
    "mastra:dev": "set -a && . ./.env && set +a && mastra dev",
    "mastra:build": "mastra build",
    "predev": "lsof -i :6445 | grep LISTEN | awk '{print $2}' | xargs kill -9",
    "dev": "node --watch -r tsconfig-paths/register --import tsx src/index.ts",
    "prebuild": "rm -rf src/generated && rm -rf dist && yarn generate",
    "build": "tsc && mkdir -p dist/src/generated && cp -R src/generated/* dist/src/generated/ && tsc-alias",
    "start": "node dist/src/index.js",
    "generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/prisma/seed.ts",
    "db:reset": "prisma migrate reset --force --skip-seed",
    "seed:vehicles": "node -r dotenv/config dist/src/scripts/seed-vehicles.js",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@aws-sdk/client-s3": "^3.969.0",
    "@aws-sdk/lib-storage": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.969.0",
    "@google/genai": "^1.37.0",
    "@google/generative-ai": "^0.24.1",
    "@mastra/core": "^1.0.4",
    "@mastra/memory": "^1.0.0",
    "@mastra/pg": "^1.0.0",
    "@mastra/upstash": "^1.0.0",
    "@prisma/client": "^5.7.1",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/jsonwebtoken": "^9.0.10",
    "@upstash/qstash": "^2.8.4",
    "@upstash/redis": "^1.36.1",
    "ai": "^6.0.62",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.4.10",
    "chalk": "4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "graphql": "^16.8.1",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "^1.22.4",
    "graphql-shield": "^7.6.5",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "music-tempo": "^1.0.3",
    "nexus": "^1.3.0",
    "nexus-prisma": "^2.0.8",
    "pusher": "^5.3.2",
    "resend": "^6.6.0",
    "uuid": "^13.0.0",
    "wav-decoder": "^1.3.0",
    "zod": "^4"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@prisma/config": "^7.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^30.1.1",
    "mastra": "^1.0.1",
    "prisma": "^5.7.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.6.2",
    "typescript": "^5.9.3"
  },
  "prisma": {
    "schema": "../../shared/prisma/schema.prisma"
  }
}
